Host tnode01 192.168.222.201
  HostName 192.168.222.201
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/vagrant/.ssh/insecure_private_key
  IdentitiesOnly yes
  LogLevel FATAL

